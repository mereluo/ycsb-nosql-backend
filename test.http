### Get databaseOption
GET http://localhost:8080/api/dbOption

### Create databaseOption
POST http://localhost:8080/api/dbOption
Content-Type: application/json

{
  "database": {"database": "MongoDB"},
  "test": "yes"
}


### Get dbConfig
GET http://localhost:8080/api/dbConfig

### Create dbConfig
POST http://localhost:8080/api/dbConfig
Content-Type: application/json

{
  "description": "Sample DBConfig",
  "isTransactional": true,
  "platform": "Sample Platform",
  "numOfNodes": 3,
  "numOfRegions": 2,
  "isMultiRegion": true,
  "databaseOption" : {"database": "MongoDB"}
}

### Get testConfig
GET http://localhost:8080/api/testConfig

### Create testConfig
POST http://localhost:8080/api/testConfig
Content-Type: application/json

{
  "concurrencyLevel": 64,
  "recordCounts": 3,
  "commandLine": "sample commandLine",
  "dbConfig": {"id" : 1}
}


### Get workload
GET http://localhost:8080/api/workload

### Get workloadB
GET http://localhost:8080/api/workloadB

### Create a new workload
POST http://localhost:8080/api/workload
Content-Type: application/json

{
  "opsPerSec": 100.0,
  "readMeanLatency": 10.5,
  "readMaxLatency": 20.5,
  "readP95": 15.5,
  "readP99": 25.5,
  "updateMeanLatency": 8.5,
  "updateMaxLatency": 18.5,
  "updateP95": 13.5,
  "updateP99": 23.5,
  "timeSeries": {
    "data": {
      "entry1": {
        "time": [1, 2, 3],
        "latency": [10.5, 15.2, 20.0]
      },
      "entry2": {
        "time": [4, 5, 6],
        "latency": [18.3, 22.1, 25.8]
      }
    }
  },
  "testConfig": {"id" :  1}
}

### Create a new workload using all fields
POST http://localhost:8080/api/workload/save
Content-Type: application/json

{
  "workloadType": "A",
  "opsPerSec": 100.0,
  "readMeanLatency": 10.5,
  "readMaxLatency": 20.5,
  "readP95": 15.5,
  "readP99": 25.5,
  "updateMeanLatency": 8.5,
  "updateMaxLatency": 18.5,
  "updateP95": 13.5,
  "updateP99": 23.5,
  "timeSeries": {
    "data": {
      "entry1": {
        "time": [1, 2, 3],
        "latency": [10.5, 15.2, 20.0]
      },
      "entry2": {
        "time": [4, 5, 6],
        "latency": [18.3, 22.1, 25.8]
      }
    }
  },

  "concurrencyLevel": 64,
  "recordCounts": 3,
  "commandLine": "sample commandLine",

  "description": "Sample DBConfig",
  "transactional": true,
  "platform": "Sample Platform",
  "numOfNodes": 3,
  "numOfRegions": 2,
  "multiRegion": true,

  "database": "MongoDB"
}
